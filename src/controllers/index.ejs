    <style>
        body, .container, .table{
            font-size: 10px !important;
            width: 2.598px !important;
        }
    </style>
    <title>PDF</title>
  </head>
  <body>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="row">
                    <div class="col-6 tex-left">
                        nome empresa
                        <div class="col-12 text-right">
                            data
                        </div>
                    </div>
                    <div class="col-6"></div>
                </div>
            </div>
        </div>    
    <div class="row">
        <div class="col-12">
            <table class="table " >
                <thead>
                    <tr>
                    <th>Solicitante</th>
                    <th>Referencia</th>
                    <th>Tal√£o</th>
                    <th>Quantidade</th>
                    <th>Valor bolsa</th>
                    <th>Data Entrada</th>
                    <th>Valor a receber</th>
                    </tr>
                </thead>
                <tbody>
                    <% bead.forEach(beads => {%>
                    <tr>
                        <td><%= beads.companies.companyName%></td>
                        <td><%= beads.reference%></td>
                        <td><%= beads.patch%></td>
                        <td><%= beads.amount%></td>
                        <td><%= beads.value%></td>
                        <td><%= beads.dateEntry%></td>
                        <td><%= beads.valueTotal%></td>
                    </tr>
                    <% }) %>         
                </tbody>
            </table>
        </div>
    </div>
</div>

